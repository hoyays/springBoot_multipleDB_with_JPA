<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>ID 찾기</title>
		<link rel="stylesheet" href="/css/member/find_id.css">
	</head>
	<body>
		<!-- header include -->
		<%@ include file="../header.jsp" %>
		<!-- /header include -->
	
		<div id="title_area">
			<h3>아이디 찾기</h3>
			<h1>등록된 회원정보로<br>아이디를 찾으실 수 있습니다.</h1>
		</div>
	
		<div id="findInfo">
			<form action="">
				<table>
					<tbody>
						<tr>
							<th>이름</th>
							<td>
								<input type="text" id="" name="" placeholder="한글 또는 영문으로 입력해 주세요!">
							</td>
						</tr>
						<tr>
							<th>E-mail</th>
							<td>
								<input type="text" id="" name="" placeholder="이메일주소를 입력하세요!">
							</td>
						</tr>
					</tbody>
				</table>		
			</form>
		</div>
		
		<div id="btn_area">
			<div id="btn_body">
				<a href="/"><input type="button" id="goList" name="" value="취소"></a>
				<input type="button" id="goSave" name="" value="확인">
			</div>
		</div>
	
		<!-- footer include -->
		<div id="footer_area">
			<%@ include file="../footer.jsp" %>
		</div>		
		<!-- /footer include -->
		
	</body>
</html>